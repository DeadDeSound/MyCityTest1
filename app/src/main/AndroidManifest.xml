                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.nezar.myamakentest1.MainActivity" />
        </activity>
        <activity
            android:name=".banks.Markets"
            android:label="@string/title_activity_markets"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.nezar.myamakentest1.MainActivity" />
        </activity>
        <activity
            android:name=".banks.Cafe"
            android:label="@string/title_activity_cafe"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.nezar.myamakentest1.MainActivity" />
        </activity>
        <activity
            android:name=".banks.Governmental"
            android:label="@string/title_activity_governmental"
